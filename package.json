{
	"name": "@asymmetrik/leaflet-d3",
	"artifactName": "leaflet-d3",
	"moduleName": "leafletD3",
	"description": "Custom d3 layers for leaflet",
	"version": "4.4.0",
	"author": "Asymmetrik, Ltd.",
	"copyright": "Copyright (c) 2007-2019 Asymmetrik Ltd, a Maryland Corporation",
	"license": "MIT",
	"scripts": {
		"build": "npm run bundle",
		"prebuild": "npm run lint",
		"prepare": "npm run bundle",
		"bundle": "npm run rollup && npm run uglify",
		"lint": "eslint 'src/**/*.js'",
		"rollup": "rollup -c rollup.config.js",
		"uglify": "uglifyjs --comments 'license' -o ./dist/leaflet-d3.min.js -- ./dist/leaflet-d3.js",
		"watch": " watch 'npm run build' ./src"
	},
	"main": "dist/leaflet-d3.js",
	"module": "index.js",
	"typings": "index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/Asymmetrik/leaflet-d3.git"
	},
	"peerDependencies": {
		"d3": "5",
		"d3-hexbin": "0.2",
		"leaflet": "1"
	},
	"devDependencies": {
		"d3": "5.9",
		"@types/d3": "5.7",
		"d3-hexbin": "0.2",
		"leaflet": "1.5",
		"@types/leaflet": "1.4",
		"eslint": "6",
		"rollup": "1",
		"uglify-js": "3",
		"watch": "1"
	}
}
